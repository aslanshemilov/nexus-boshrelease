-Xms<%= p("nexus.heap_size") %>
-Xmx<%= p("nexus.heap_size") %>
-XX:MaxDirectMemorySize=<%= p("nexus.max_direct_memory_size") %>
-XX:+UnlockDiagnosticVMOptions
-XX:+UnsyncloadClass
-XX:+LogVMOutput
-XX:LogFile=/var/vcap/sys/log/nexus/jvm.log
-Djava.net.preferIPv4Stack=true
-Dkaraf.home=.
-Dkaraf.base=.
-Dkaraf.etc=etc/karaf
-Djava.util.logging.config.file=etc/karaf/java.util.logging.properties
-Dkaraf.data=<%= p("nexus.data_dir") %>
-Djava.io.tmpdir=/var/vcap/data/sys/tmp
-Dkaraf.startLocalConsole=false